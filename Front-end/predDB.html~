		    jQuery(function($, undefined) {
		        $('#term_demo').terminal(function(command, term) {
		        	mydat = CSVtoSQLFormat(masterData, "dha")
			        console.log(mydat["dataTable"])
			        console.log(mydat["columnDefs"])
		        if (command !== '') {
		        try {
		        	var tableData = mydat["dataTable"]/* { 
		        	        Invoice  : [ { "id": 1, "total": 100, "custId": 10 }, 
		        	                     { "id": 2, "total": NaN, "custId": 20 }, 
		        	                     { "id": 3, "total": 300, "custId": 10 }, 
		        	                     { "id": 4, "total": 400, "custId": 20 } ]
		        	    }; */
		        	var columnDefs = mydat["columnDefs"]/* {
		        		        Invoice  : { id          : { type: "String" },
		        		                     total       : { type: "Number" },  
		        		                     custId      : { type: "String" } }
		        		    }; */
				
			    var queryLang = TrimPath.makeQueryLang(columnDefs);
			    if (command.split(' ')[0]=="SELECT")
			    	{
			    	   var statement = queryLang.parseSQL(command);
					   var result = statement.filter(tableData);
					   alert(result)
			    	}
			    else
			    	{
			    	inferCommandParsed = command.split(' ');
			    	var confidence = inferCommandParsed[$.inArray("CONFIDENCE", inferCommandParsed) + 1]
			    	var resolution = inferCommandParsed[$.inArray("RESOLUTION", inferCommandParsed) + 1]
			    	/* send these two to backend and retrieve new table */
			    	tableData =  { 
		        	        Invoice  : [ { "id": 1, "total": 100, "custId": 10 }, 
		        	                     { "id": 2, "total": 100, "custId": 20 }, 
		        	                     { "id": 3, "total": 300, "custId": 10 }, 
		        	                     { "id": 4, "total": 400, "custId": 20 } ]}
			    	command = command.replace("INFER","SELECT");
			    	command = command.substring(0, command.indexOf("WITH") - 1); 
			    	var statement = queryLang.parseSQL(command);
					var result = statement.filter(tableData);  
			    	}
			    
			    /* alert(command.split(' ')[0]=="SELECT")  */
			    var out = "";
			    for (var r = 0; r < result.length; r++) {
			        for (var c in result[r])
			            /* out += c + ": " + result[r][c] + ", "; */
			        	out += result[r][c] + ", ";
			        out += "\n";
			    }
			    var aDataSet = $.csv.toArrays(out); 
			    
			    var columns = new Array();
			    var counter = 0;
			    for (var c in result[0]){
			    	columns[counter] = { "sTitle": c , "sClass": "center"}
			    	counter += 1
			    }
			    
			    /*storing the tables*/
			    datTemp.push([[columns, aDataSet]]) 
			    alert(datTemp)
			    
	     		$('#dynamic').html( '<table cellpadding="0" cellspacing="0" border="0" class="display" id="example"></table>' );
				$('#example').dataTable( {
					"aaData": aDataSet,
					"aoColumns": columns
				} );
			    document.getElementById('result').innerHTML = out; 
			    /* document.write(lines);  */
		        if (result !== undefined) {
		        /* term.echo(new String(result)); */
		        }
		        } catch(e) {
		        term.error(new String(e));
		        }
		        } else {
		        /* term.echo(''); */
		        }
		        }, {
		        greetings: '',
		        name: 'SQL_demo',
		        height: 100,
		        width: 500,
		        prompt: 'SQL Command>'});
		        });
			
		function ProcessFiles(files_input) {
			for (var file_index = 0; file_index < files_input.length; file_index++) {
				var reader = new FileReader();
				reader.onload = function(e) {
					LoadToDatabaseTheCSVData(e.target.result)
				}
				reader.readAsBinaryString(files_input[file_index])	
			}
		}
		    
